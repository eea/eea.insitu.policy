<configure
  xmlns="http://namespaces.zope.org/zope"
  xmlns:browser="http://namespaces.zope.org/browser"
  i18n_domain="eea">

  <browser:page
    name="import_insitu_reports"
    for="*"
    template="view.pt"
    class=".views.ImportInsituReports"
    permission="zope2.View"
    />

  <browser:page
    name="import_insitu_report_dates"
    for="*"
    template="view.pt"
    class=".views.ImportInsituReportsDates"
    permission="zope2.View"
    />
</configure>
